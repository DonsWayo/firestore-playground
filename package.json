{
  "name": "firestore-playground",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Firestore Playground",
    "appId": "com.donswayo.firestoreplayground",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "category": "app.category.developertools",
      "target": [
        "zip",
        "dmg"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:DonsWayo/firestore-playground.git"
  },
  "author": {
    "name": "DonsWayo",
    "email": "djwayomix@gmail.com"
  },
  "license": "SEE LICENSE",
  "bugs": {
    "url": "https://github.com/DonsWayo/firestore-playground.git/issues"
  },
  "homepage": "https://github.com/DonsWayo/firestore-playground",
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/polyfill": "7.10.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@hot-loader/react-dom": "16.13.0",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-hot-loader": "4.1.1",
    "@types/webdriverio": "5.0.0",
    "@types/webpack-env": "1.15.2",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "css-loader": "4.2.1",
    "electron": "9.2.1",
    "electron-builder": "22.8.0",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "image-webpack-loader": "6.0.0",
    "node-sass": "7.0.0",
    "react-hot-loader": "4.12.21",
    "sass-loader": "9.0.3",
    "source-map-loader": "1.0.2",
    "spectron": "11.1.0",
    "style-loader": "1.2.1",
    "typescript": "4.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@risingstack/react-easy-state": "6.3.0",
    "@types/react-router-dom": "5.1.5",
    "firebase": "7.19.0",
    "fork-ts-checker-webpack-plugin": "5.1.0",
    "monaco-editor-webpack-plugin": "1.9.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-monaco-editor": "0.37.0",
    "react-router-dom": "5.2.0"
  }
}
