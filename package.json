{
  "name": "firestore-playground",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Firestore Playground",
    "appId": "com.donswayo.firestoreplayground",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ],
    "linux": {
      "target": "deb"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "category": "app.category.developertools",
      "target": [
        "zip",
        "dmg"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:DonsWayo/firestore-playground.git"
  },
  "author": {
    "name": "DonsWayo",
    "email": "djwayomix@gmail.com"
  },
  "license": "SEE LICENSE",
  "bugs": {
    "url": "https://github.com/DonsWayo/firestore-playground.git/issues"
  },
  "homepage": "https://github.com/DonsWayo/firestore-playground",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/polyfill": "7.8.7",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@hot-loader/react-dom": "16.13.0",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-hot-loader": "4.1.1",
    "@types/webdriverio": "5.0.0",
    "@types/webpack-env": "1.15.1",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "electron": "8.2.4",
    "electron-builder": "22.4.1",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.0.4",
    "image-webpack-loader": "6.0.0",
    "node-sass": "4.13.1",
    "react-hot-loader": "4.12.20",
    "sass-loader": "8.0.2",
    "source-map-loader": "0.2.4",
    "spectron": "10.0.1",
    "style-loader": "1.1.3",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@material-ui/core": "4.9.9",
    "@material-ui/icons": "4.9.1",
    "@risingstack/react-easy-state": "6.2.1",
    "@types/react-router-dom": "5.1.3",
    "firebase": "7.14.0",
    "fork-ts-checker-webpack-plugin": "4.1.2",
    "monaco-editor-webpack-plugin": "1.9.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-monaco-editor": "0.35.0",
    "react-router-dom": "5.1.2"
  }
}
